#include <iostream>

using namespace std;

int main(void){
    int a[1000] = {0};
    int i = 0,n = 0,key,val;
    cin>>n;
    for(int i = 0; i < n; i ++){
        cin>>key>>val;
        a[key] += val;
    }
    for(i = 0; i < 1000 ; i ++){
    	if(a[i] != 0)
    		cout<<i<<' '<<a[i]<<endl;
    }
    return 0;
}
